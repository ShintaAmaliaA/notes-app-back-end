{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "indent": "off",
        "eol-last": 0,
        "no-trailing-spaces" :"off",
        "no-undef" : "off"
    }
}
